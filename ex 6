#include <iostream> 
using namespace std;


int main()
{
    float x,y;
    
    
    cout << "Digite um numero: ";
    cin >> x;
    
    cout << "digite o segundo numero: ";
    cin >> y;
    
    if (x == y){
        
        cout << "Seus numeros são iguais ";
        
    } else if(x>y) {
        
        cout << "Esse é o maior numero: " << x;
        
        
    } else {
        
        cout << "Esse é o maior numero: " << y;
        
        
    }
    

    return 0;
}
